{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "bwrice/laravel-job-chain-groups": "dev-master",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/horizon": "^3.2",
        "laravel/nova": "*",
        "laravel/passport": "^7.0",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.0",
        "laravel/telescope": "^2.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "markrogoyski/math-php": "0.*",
        "nothingworks/blade-svg": "^0.3.4",
        "rollbar/rollbar-laravel": "^6.0",
        "spatie/enum": "^2.1",
        "spatie/laravel-backup": "^6.10",
        "spatie/laravel-event-sourcing": "^1.0",
        "spatie/laravel-permission": "^3.0",
        "spatie/laravel-query-builder": "^2.0",
        "spatie/laravel-sluggable": "^2.1",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^5.5",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phploc/phploc": "^5.0",
        "phpunit/phpunit": "^7.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
